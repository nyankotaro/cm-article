{
  "title": "For Japanese （日本語環境向けの設定）",
  "rules": [
    {
      "description": "かなを単体で押したときに、かなキーを送信する。他のkeyと併用された場合はright_commandを送信する",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "japanese_kana",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_kana"
            }
          ]
        }
      ]
    }
  ]
}